			]
		},
		"SchemaChanges": {
			"class": "MediaWiki\\CheckUser\\HookHandler\\SchemaChangesHandler"
		},
		"ToolLinks": {
			"class": "MediaWiki\\CheckUser\\HookHandler\\ToolLinksHandler",
			"services": [
				"PermissionManager",
				"SpecialPageFactory",
				"LinkRenderer",
				"UserIdentityLookup",
