<li>
	{{#canPerformBlocks}}
		<input type="checkbox" name="users[]" value="{{userText}}"/>
	{{/canPerformBlocks}}
	<span class="mw-checkuser-user-link{{#userLinkClass}} {{userLinkClass}}{{/userLinkClass}}">
		{{{userLink}}}
	</span>
	{{{userToolLinks}}}
	{{{userLinks}}}
	{{{centralAuthLink}}}
	{{{globalBlockLink}}}
	<span class="mw-changeslist-links">{{timeRange}}</span>
	<strong class="mw-checkuser-edits-count">{{editCount}}</strong><br/>
	{{#flags}}
		{{{ . }}}
	{{/flags}}
	<ol>
		{{#infoSets}}
			<li>
				<span class="mw-checkuser-ip">{{{ipLink}}}</span>{{#xff}}&#160;&#160;<strong>XFF</strong>: <span class="mw-checkuser-xff{{#xffTrusted}} mw-checkuser-xff-trusted{{/xffTrusted}}">{{{ . }}}</span>{{/xff}}
			</li>
		{{/infoSets}}
	</ol><br><ol>
		{{#agentsList}}
			<li>
				<i dir="ltr" class="mw-checkuser-agent">{{ . }}</i>
			</li>
		{{/agentsList}}
	</ol>
</li>
